<h1>Listing poster_statuses</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @poster_statuses.each do |poster_status| %>
  <tr>
    <td><%= poster_status.name %></td>
    <td><%= link_to 'Show', poster_status %></td>
    <td><%= link_to 'Edit', edit_poster_status_path(poster_status) %></td>
    <td><%= link_to 'Destroy', poster_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Poster status', new_poster_status_path %>
